    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server Attach",
            "type": "node",
            "request": "attach",
            "protocol": "inspector",
            "address": "localhost",
            "port": 9229
          },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9230,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\hello-node.js"
        }
    ]
}